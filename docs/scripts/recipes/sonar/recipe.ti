# Install Java JDK
java.version = oracleJDK8

# Install PostgreSQL Database Server
postgresql.server.version = 9.6
postgresql.server.host = localhost
postgresql.server.port = 5432
postgresql.server.remote.access = no

# Install NGINX web server
nginx

# Install SonarQube
sonar.version = 6.7.6
sonar.postgresql.host = localhost
sonar.postgresql.port = 5432
sonar.nginx.domain = sonar.local
sonar.nginx.host = localhost:9000
